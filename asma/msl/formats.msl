    source V1 V1
    source SX V2 B2 D2
    source M3 M3


# VRX
# ---
#    
#     0     4     8     12    16    20    24    28    32    36    40    44   47
#     +-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+
#     |     OP    | V1  | X2  | B2  |        D2       | M3  | RXB |    XOP    |
#     +-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+
#
# VRX: MNEMONIC 
# (VECTOR LOAD LOGICAL ELEMENT AND ZERO)
format VRX
    length 6
    xopcode 40 47
    mach V1 8  11
    mach X2 12 15
    mach B2 16 19
    mach D2 20 31
    mach M3 32 35
    mach RXB 36 39
    source V1 V1
    source SX X2 B2 D2
    source M3 M3


# VRX - without M3
# ---
#    
#     0     4     8     12    16    20    24    28    32    36    40    44   47
#     +-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+
#     |     OP    | V1  | X2  | B2  |        D2       |0000 | RXB |    XOP    |
#     +-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+
#
# VRX1: MNEMONIC V1,D2(X2,B2)
# (VECTOR LOAD)
format VRX1
    length 6
    xopcode 40 47
    mach V1 8  11
    mach X2 12 15
    mach B2 16 19
    mach D2 20 31
    mach RXB 36 39
    source V1 V1
    source SX X2 B2 D2


#     0     4     8     12   15
#     +-----+-----+-----+-----+
#     |     OP    | XOP | R1  |
#     +-----+-----+-----+-----+
#
# ER: MNEMONIC R1
#
#  Note: XOP is used for the implied BRANCH ON CONDITIION mask
#
# Extended mnemonic format for BCR
format ER
    length 2
    xopcode 8 11
    mach R1 12 15
    source R1 R1


#     0     4     8     12    16    20    24    28   31
#     +-----+-----+-----+-----+-----+-----+-----+-----+
#     |     OP    | XOP | X1  | B1  |       D1        |
#     +-----+-----+-----+-----+-----+-----+-----+-----+
#
# ERXB: MNEMONIC D1(X1,B1)
#
#  Note: XOP is used for implied BRANCH ON CONDITION mask
#
# Extended mnemonic format for BC (and a few other uses)
format ERXB
    length 4
    xopcode 8 11
    mach X1 12 15
    mach B1 16 19
    mach D1 20 31
    source SX X1 B1 D1


#     0     4     8     12    16    20    24    28   31
#     +-----+-----+-----+-----+-----+-----+-----+-----+
#     |     OP    |    XOP    |         RELI1         |
#     +-----+-----+-----+-----+-----+-----+-----+-----+
#
# ERIC: MNEMONIC RELI1
#
#  Note: XOP includes the implied BRANCH RELATIVE ON CONDITION mask
#
# Extended mnemonic format for BRC
format ERIC
    length 4
    xopcode 8 15
    mach RELI1 16 31 signed
    source RELI1 RELI1


#     0     4     8     12    16    20    24    28    32    36    40    44   47
#     +-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+
#     |     OP    |    XOP    |                     RELI1                     |
#     +-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+
#
# ERILC: MNEMONIC RELI1
#
#  Note: XOP includes the implied BRANCH RELATIVE ON CONDITION LONG mask
#
# Extended mnemonic format for BRCL
format ERILC
    length 6
    xopcode 8 15
    mach RELI1 16 47 signed
    source RELI1 RELI1


# VRI-a - Extended Mnemonic for RI2
# -----
#  
#     0     4     8     12    16    20    24    28    32    36    40    44   47
#     +-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+
#     |     OP    | V1  |0000 |          RI2          |0000 | RXB |    XOP    |
#     +-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+-----+
#
# EVRIA: MNEMONIC V1
#
format EVRIA
    length 6
    xopcode 40 47
    mach V1 8 11
    mach RI2 16 31 signed fixed
    mach RXB 36 39
    source V1 V1