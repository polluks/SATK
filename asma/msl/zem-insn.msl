#
#   RISBHG (EC5D) EXTENDED MNEMONICS
#
inst SLLHH  EC5D  ERIEF-B E   # SHIFT LEFT LOGICAL (HIGH <- HIGH)
    fixed  I3 00
    filter I4 31MINUSZ  # I3 is redirected to I4 by the ERIEF-B format
    #      I5 is directed from I3 without alteration by the ERIEF-B format

inst SLLHL EC5D   ERIEF-B E   # SHIFT LEFT LOGICAL (HIGH <-LOW)
    fixed  I3 00
    filter I4 31MINUSZ  # I3 is redirected to I4 by the ERIEF-B format
    filter I5 32PLUS    # I3 is redirected to I5 by the ERIEF-B format

inst SRLHH EC5D   ERIEF-C E   # SHIFT LEFT LOGICAL (HIGH <-LOW)
    #      I3 is directed from I3 without alteration by the ERIEF-C format
    fixed  I4 9F  # Treat Z bit as one
    filter I5 MINUS     # I3 is redirected to I5 by the ERIEF-C format

inst SRLHL EC5D   ERIEF-C E   # SHIFT LEFT LOGICAL (HIGH <-LOW)
    #      I3 is directed from I3 without alteration by the ERIEF-C format
    fixed  I4 9F  # Treat Z bit as one
    filter I5 32MINUS   # I3 is redirected to I5 by the ERIEF-C format


